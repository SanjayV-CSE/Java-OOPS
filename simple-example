class Car {
    String brand;
    int speed;

    void displayDetails() {
        System.out.println("Brand: " + brand);
        System.out.println("Speed: " + speed + " km/h");
    }
}

public class Main {
    public static void main(String[] args) {
        Car car1 = new Car();
        
        car1.brand = "Toyota";
        car1.speed = 120;
        car1.displayDetails();
    }
}
